{
    "name": "javaabu/customs-api",
    "description": "PHP SDK for interacting with the Maldives Customs Service API",
    "homepage": "https://github.com/Javaabu/customs-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Arushad Ahmed",
            "email": "arushad@javaabu.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "guzzlehttp/guzzle": "~7.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Customs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Customs\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\Customs\\CustomsServiceProvider"
            ],
            "aliases": {
                "Customs": "Javaabu\\Customs\\Facades\\Customs"
            }
        }
    },
    "minimum-stability": "stable"
}
